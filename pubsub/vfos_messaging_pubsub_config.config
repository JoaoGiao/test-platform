loopback_users.guest = false
listeners.tcp.default = 5672
hipe_compile = false
management.listener.port = 15672
management.listener.ssl = false
management.load_definitions = /etc/rabbitmq/definitions.json
auth_backends.1 = http
auth_http.http_method = post
auth_http.user_path = http://broker-auth-adapter:5879/auth/user
auth_http.vhost_path = http://broker-auth-adapter:5879/auth/vhost
auth_http.resource_path = http://broker-auth-adapter:5879/auth/resource
auth_http.topic_path = http://broker-auth-adapter:5879/auth/topic
